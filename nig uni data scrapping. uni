   "source": [
    "#Cleaning and Adding Data into the columns \n",
    "\n",
    "\n",
    "\n",
    "for row in column_data[1:]:\n",
    "    row_data = row.find_all('td')\n",
    "    \n",
    "    individual_row_data = [column.get_text().strip() for column in row_data]\n",
    "    data.append(individual_row_data)\n",
    "    length = len(df)\n",
    "    df.loc[length] = individual_row_data\n",
    "    \n",
    "\n",
